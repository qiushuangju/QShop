    "mp-weixin" : {
        "appid" : "wxa0ac1867dadc4cbd",
        "setting" : {
            // 是否检查安全域名和 TLS 版本
            "urlCheck" : true,
            // es6转es5
            "es6" : false,
            // 上传代码时自动压缩
            "minified" : true,
